{
  "name": "hypefeed-worker",
  "version": "0.1.0",
  "description": "AI YouTube channels RSS feed aggregation service on Cloudflare Workers",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "build": "bun run build:worker",
    "build:worker": "esbuild src/index.ts --bundle --outfile=dist/worker.js --format=esm --target=es2022",
    "type-check": "tsc --noEmit",
    "test": "bun test"
  },
  "dependencies": {
    "@prisma/adapter-d1": "^6.16.2",
    "@prisma/client": "^6.16.2",
    "date-fns": "^4.1.0",
    "fast-xml-parser": "^4.5.3",
    "hono": "^4.9.7"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250913.0",
    "@types/node": "^22.18.3",
    "esbuild": "^0.24.2",
    "prisma": "^6.16.2",
    "typescript": "^5.9.2",
    "wrangler": "^4.36.0"
  },
  "keywords": [
    "cloudflare-workers",
    "rss",
    "youtube",
    "ai",
    "aggregation",
    "hono"
  ],
  "author": "HypeFeed",
  "license": "MIT"
}